{
  "name": "deep-sql-research",
  "private": true,
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "type-check": "turbo run type-check",
    "clean": "turbo run clean",
    "studio": "turbo run studio --filter=agent",
    "generate-video": "bun run packages/agent/src/index.ts \"Analyze Netflix content performance to understand what makes a show successful\" packages/agent/netflixdb.sqlite > template.json && bun run apps/renderer/src/cli.ts template.json"
  },
  "devDependencies": {
    "@types/bun": "^1.2.14",
    "turbo": "^2.6.1",
    "typescript": "^5.8.3"
  },
  "packageManager": "bun@1.3.1",
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.68",
    "@ai-sdk/openai-compatible": "^1.0.27",
    "ai": "^5.0.93"
  }
}
